#compdef ukigumo-agent

_arguments -n : \
  '(--server-url)--repo=[ukigumo-server URL]' \
  '(--work_dir)--work_dir=[project name]:WorkDir:_files -/' \
  '(--host)--host=[Bind host]' \
  '(--port)--port=[Bind port]' \
  '*: :()'

